{
  "name": "depense-diviser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite --host 0.0.0.0 --port 5000",
    "dev:routify": "routify",
    "build": "routify -b && vite build",
    "preview": "vite preview --host 0.0.0.0",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "copy:types": "cp ../vitrak/node_modules/@generated/ ./src/lib/ -r"
  },
  "devDependencies": {
    "@roxi/routify": "^2.18.5",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.51.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.5"
  },
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.8",
    "@urql/svelte": "^1.3.3",
    "bulma": "^0.9.3",
    "graphql": "^16.4.0",
    "remixicon": "^2.5.0",
    "svelte-forms-lib": "github:sphingu/svelte-forms-lib#1.0.0"
  }
}